package learning1;

public class user {
	String name;
	String course;
	avaliacao notas;
	
	public user(String name, String course, avaliacao notas) {
		super();
		this.name = name;
		this.course = course;
		this.notas = notas;
	}


	void data(){
		System.out.println("waca waca UwU");
		System.out.println("---------------");
		System.out.println("Nome: " + name);
		System.out.println("Curso: " + course);
		System.out.println("Nome: " + notas.mediaAritmetica());
		System.out.println("Nome: " + notas.mediaPonderada());
	}
}
